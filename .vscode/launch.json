{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++ Build and debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/WebServer/WebServer",
            "args": [
                // "-t 3",
                // "-p 88",
                // "-l /home/ruanyuan/server/WebServer"
            ],
            "sourceFileMap":{
                "/build/glibc-eX1tMB": "/usr/src/glibc"
            },
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "miDebuggerPath": "/home/ruanyuan/server/gdb",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build"
            // "miDebuggerPath": "/usr/bin/gdb"
            // "miDebuggerPath": "/home/ruanyuan/serverWebServer/.vscode/gdb.sh"  报错，因为该shell文件无执行权限
        }
    ]
}